/*============================================================================
  #Breakpoint and Grid Variables
==============================================================================*/
$viewportIncrement: 1px;

$small: 480px;
$medium: 768px;
$large: 769px;
$xlarge: 960px;
$xxlarge: 1200px;

$postSmall: $small + $viewportIncrement;
$preMedium: $medium - $viewportIncrement;
$preLarge: $large - $viewportIncrement;

/*================ The following are dependencies of csswizardry grid ================*/
$breakpoints: (
  'small' '(max-width: #{$small})',
  'medium' '(min-width: #{$postSmall}) and (max-width: #{$medium})',
  'medium-down' '(max-width: #{$medium})',
  'large' '(min-width: #{$large})',
  'xlarge-down' '(max-width: #{$xlarge})',
  'xlarge' '(min-width: #{$xxlarge})'
);
$breakpoint-has-widths: ('small', 'medium', 'medium-down', 'large', 'xlarge', 'xlarge-down');
$breakpoint-has-push:  ('medium', 'medium-down', 'large', 'xlarge', 'xlarge-down');
$breakpoint-has-pull:  ('medium', 'medium-down', 'large', 'xlarge', 'xlarge-down');

