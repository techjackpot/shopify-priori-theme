        content: '';
      }

      &:before {
        left: 30px;

        width: 3px;
        height: 730px;
        background: $grey-txt;
      }

      &:after {
        left: 21px;
        width: 22px;
        height: 22px;

        border-radius: 100%;
        background: #f6f3f1;
        border: 3px solid $grey-txt;
      }

      &:first-child {
        &:before { height: 1200px; }

        &:after,
        &:before {
          top: -350px;
        }
      }

      &:nth-child(2) {
        &:after {
          top: auto;
          bottom: -120px;
        }
      }

      &:last-child {
        margin-bottom: 0;

        &:before { display: none; }
      }
